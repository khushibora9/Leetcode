class Solution {
    public int[] findPeakGrid(int[][] m) {
        int r[]=new int [2];
        int c=m[0].length;
        int v=0; 
        for(int i=0;i<m.length;i++)
        {for(int j=0;j<c;j++)
        {
            // v=Math.max(v,m[i][j]);
            if(m[i][j]>v)
            {v=m[i][j];
            r[0]=i;
            r[1]=j;}}}
            return r;        
    }
}
