class Solution {
    public int searchInsert(int[] nums, int t) {
        for(int i=0;i<nums.length;i++)
        {
            if(nums[i]==t)
            {
                return i;}
                if(i!=(nums.length-1))

            {if((nums[i]<t)&&(nums[i+1]>t))
            {return (i+1);}}
            if(t < nums[0])

            {return 0;}

        }
        return nums.length;
    }
}
